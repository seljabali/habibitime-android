{
  "manifest_version": 2,
  "name": "DevBox",
  "description": "DevBox",
  "version": "1.0",
  "permissions": [
	"cookies",
	"tabs",
	"notifications",
  "clipboardRead",
  "clipboardWrite",
	"http://ajax.googleapis.com/ajax/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "devbox.html"
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}