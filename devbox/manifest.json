{
  "manifest_version": 2,
  "name": "DevBox",
  "description": "Easy access to common library XO functions.",
  // "homepage_url": "https://confluence.workday.com/TBD",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
	"cookies",
	"tabs",
	"notifications",
  "clipboardRead",
  "clipboardWrite",
	"http://ajax.googleapis.com/ajax/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "devbox.html"
  },
  "web_accessible_resources": [
    "icon.png"
  ]
}